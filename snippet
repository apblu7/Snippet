@echo off
setlocal

rem ====== 설정 ======
set WINSCP="C:\HUNESION\i-oneNGS\WinSCP.exe"
set SERVER=ubuntu@10.110.30.50
set KEY_PATH=%~dp0server_key.ppk
set WAR_FILE=ets_test.war
set LOCAL_PATH=%~dp0target\%WAR_FILE%
set REMOTE_PATH=/home/ubuntu/

rem ====== 업로드 실행 ======
%WINSCP% /console /command ^
  "open sftp://%SERVER%/ -privatekey=""%KEY_PATH%""" ^
  "put ""%LOCAL_PATH%"" ""%REMOTE_PATH%""" ^
  "exit"

pause